<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andrewseto.workoutbuddy2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CalculatorScreen"
            android:label="@string/calculator_name"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            />
        <activity android:name=".WilksScreen"
            android:label="@string/wilks_name"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            />

        <activity android:name=".BreakTimerScreen"
            android:label="@string/breaktimer_name"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            />
        <activity android:name=".WorkoutScreen"
            android:label="@string/workout_name"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            />


    </application>

</manifest>